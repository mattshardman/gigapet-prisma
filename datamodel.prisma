type Parent {
  id: ID! @id @unique
  name: String!
  email: String!
  username: String! @unique
  password: String!
  pin: String!
  img_url: String!
}

type Child {
  id: ID! @id @unique
  name: String!
  pet_name: String!
  pet_experience: Int!
  parent: Parent @relation(link: INLINE)
}

type FoodEntry {
  id: ID! @id @unique
  name: String!
  meal: String!
  category: String!
  date_added: String!
  date_updated: String!
  child_id: Child @relation(link: INLINE)
}